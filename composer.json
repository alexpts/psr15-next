{
    "name": "alexpts/psr15-next",
    "description": "Router for PSR-7 and PSR-15",
    "homepage": "https://github.com/alexpts/psr15-next",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "middleware",
        "router",
        "routing",
        "psr-7",
        "psr-15",
        "micro"
    ],
    "config": {
        "optimize-autoloader": true
    },
    "authors": [
        {
            "name": "Alexpts",
            "email": "alexpts3@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "~8.1",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "alexpts/php-tools": "^4.0",
        "alexpts/php-simple-events": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "alexpts/psr15-middlewares": "^2.1",
        "alexpts/mutable-psr7": "^0.2",
        "alexpts/http-message-parser": "^0.0.3"
    },
    "suggest": {
        "blackfire/php-sdk": "^1.30"
    },
    "autoload": {
        "psr-4": {
            "PTS\\NextRouter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PTS\\NextRouter\\Controller\\": "test/controller"
        }
    }
}
